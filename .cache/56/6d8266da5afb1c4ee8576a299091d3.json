\n    const sanitizedChildren = node.children\r\n      .map(child => cleanData(child))\r\n      .filter(child => child !== null);  // 过滤无效的子节点\r\n\r\n    if (sanitizedChildren.length > 0) {\r\n      cleanNode.children = sanitizedChildren;\r\n    }\r\n  }\r\n\r\n  return cleanNode;\r\n}\r\n\r\n// 处理原始数据，移除\"External link\"数据\r\nexport function processData(rawData) {\r\n  const rootNode = {\r\n    name: \"所有公司\",\r\n    children: []\r\n  };\r\n\r\n  rawData.forEach(category => {\r\n    if (category.category === \"External link\") return;  // 跳过\"External link\"分类\r\n\r\n    const categoryNode = {\r\n      name: category.category,\r\n      children: []\r\n    };\r\n\r\n    // 遍历子类别\r\n    if (category.subcategories && Array.isArray(category.subcategories)) {\r\n      category.subcategories.forEach(subcategory => {\r\n        if (subcategory.subcategory === \"External link\") return;  // 跳过\"External link\"子类别\r\n\r\n        const subcategoryNode = {\r\n          name: subcategory.subcategory,\r\n          children: []\r\n        };\r\n\r\n        // 遍历每个子类别的风格\r\n        if (subcategory.genres && Array.isArray(subcategory.genres)) {\r\n          subcategory.genres.forEach(genre => {\r\n            const cleanedGenre = cleanData(genre);\r\n            if (cleanedGenre && cleanedGenre.name) {\r\n              subcategoryNode.children.push(cleanedGenre);\r\n            }\r\n          });\r\n        }\r\n\r\n        // 只有有效的子类别才加入\r\n        if (subcategoryNode.children.length > 0) {\r\n          categoryNode.children.push(subcategoryNode);\r\n        }\r\n      });\r\n    }\r\n\r\n    // 只有有效的类别才加入\r\n    if (categoryNode.children.length > 0) {\r\n      rootNode.children.push(categoryNode);\r\n    }\r\n  });\r\n\r\n  return rootNode;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f6d676c048435136e5e3762640f11fda","cacheData":{"env":{}}}